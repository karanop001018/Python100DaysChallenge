names = "Karan,Omkar,Ansh,Tanisha,Ajil,Ruchit,Krish"
print(len(names))
print(names[0:5])
print(names[6:11])
print(names[12:16])
print(names[17:24])
print(names[25:29])
print(names[30:36])
print(names[37:42])

fruit = 'Mango'
len1 = len(fruit)
print("Mango is a ", len1, "letter word")
print(fruit[0:-3])
# print(fruit[0:len(fruit)-3])
print(fruit[-1:-3])  # No output
# Because it's (4:2) in positive indexing which is not possible
print(fruit[-3:-1])
